{
  "name": "yondar-mono",
  "private": true,
  "version": "0.0.0",
  "license": "See ./LICENSE",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "yarn run translate && tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "extract": "formatjs extract",
    "compile": "formatjs compile",
    "translate": "yarn extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@cmdcode/nostr-emitter": "^2.0.9",
    "@turf/turf": "^6.5.0",
    "autoprefixer": "^10.4.16",
    "latlon-geohash": "^2.0.0",
    "mapbox-gl": "^2.15.0",
    "nostr-tools": "^1.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-intl": "^6.5.5",
    "react-map-gl": "^7.0.25",
    "react-router-dom": "^6.16.0",
    "sass": "^1.63.3"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.4",
    "@types/latlon-geohash": "^2.0.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-plugin-formatjs": "^10.5.10",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
